@import '../../assets/css/variables.styl'

header
    display flex
    justify-content space-between
    align-items center
    position relative
    box-sizing border-box
    padding 0 16px
    width 100%
    height 35px
    background-color rgba(rgb(5, 97, 76), .3)
    

